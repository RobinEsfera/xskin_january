#site_admin.off-canvas-wrap.move-right .left-off-canvas-menu {
  width: 18.5%;
  position: fixed;
  transform: translate3d(0%, 0px, 0px);
  -o-transform: translate3d(0%, 0px, 0px);
  -ms-transform: translate3d(0%, 0px, 0px);
  -webkit-transform: translate3d(0%, 0px, 0px);
  -moz-transform: translate3d(0%, 0px, 0px);
  transition: transform 750ms ease 0s;
  -o-transition: transform 750ms ease 0s;
  -ms-transition: transform 750ms ease 0s;
  -webkit-transition: transform 750ms ease 0s;
  -moz-transition: transform 750ms ease 0s;
  background-color: rgba(0, 0, 0, 0.04);
}
#site_admin .left-off-canvas-menu{
  transform: translate3d(-104%, 0px, 0px);
  -o-transform: translate3d(-104%, 0px, 0px);
  -ms-transform: translate3d(-104%, 0px, 0px);
  -webkit-transform: translate3d(-104%, 0px, 0px);
  -moz-transform: translate3d(-104%, 0px, 0px);
}
.vertical-handle {
  background: rgba(0, 0, 0, 0.5) none repeat scroll 0 0;
  border-radius: 11px;
  width: 6px;
  max-height: 150px;
  transition: none;
  -o-transition: none;
  -ms-transition: none;
  -moz-transition: none;
  -webkit-transition: none;
}
.top, .bottom{
  border: 0 none !important;
  background: none !important;
}
.xskin-dashboard-menu.tab-bar a.menu-icon {
  z-index: 999;
}
.user-profile-pic {
  background-image: url("../assets/store-slider-image.jpg");
  background-repeat: no-repeat;
  background-size: 350px auto;
  overflow: hidden;
  padding: 25px;
  width: 100%;
  position: relative;
}
.user-profile-pic::before {
  background-color: rgba(0, 0, 0, 0.65);
  content: "";
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.user-profile-pic .profile-pic {
  box-shadow: 0 0 0 3px #fff;
  height: 50px;
  position: relative;
  width: 50px;
}
.user-name {
  color: #fff;
  float: left;
  font-size: 16px;
  font-weight: 700;
  line-height: 1;
  margin: 9px 0 0;
  padding: 0 15px;
  position: relative;
  text-transform: uppercase;
  width: 65%;
}
.user-name span {
  color: #cccccc;
  display: inline-block;
  font-family: arial;
  font-size: 10px;
  font-weight: 100;
  line-height: 1px;
  width: 100%;
}
ul.dashboard-nav{
  padding: 0 0 35px;
}
ul.dashboard-nav > li a {
  color: #666;
  display: block;
  font-size: 16px;
  font-weight: 700;
  height: 40px;
  line-height: 40px;
  padding: 0 15px;
  width: 100%;
}
ul.dashboard-nav > li.select a,
ul.dashboard-nav > li a:hover{
  background-color: rgba(0, 0, 0, 0.06);
  padding-left: 25px;
}
#site_admin.move-right > .inner-wrap {
  transform: none;
  -o-transform: none;
  -ms-transform: none;
  -webkit-transform: none;
}
#site_admin.off-canvas-wrap.move-right .inner-wrap .main-section{
	padding-left: 18.5%;
	transition: padding 500ms ease-in-out 0.0s;
	-o-transition: padding 500ms ease-in-out 0.0s;
	-ms-transition: padding 500ms ease-in-out 0.0s;
	-moz-transition: padding 500ms ease-in-out 0.0s;
	-webkit-transition: padding 500ms ease-in-out 0.0s;
}
#site_admin.off-canvas-wrap .inner-wrap .main-section{
	padding-left: 0;
	transition: padding 500ms ease-in-out 0.0s;
	-o-transition: padding 500ms ease-in-out 0.0s;
	-ms-transition: padding 500ms ease-in-out 0.0s;
	-moz-transition: padding 500ms ease-in-out 0.0s;
	-webkit-transition: padding 500ms ease-in-out 0.0s;
}
#site_admin.off-canvas-wrap.move-right .exit-off-canvas{
	box-shadow: none;
}
.xskin-dashboard-menu.tab-bar {
  background-color: rgba(0, 0, 0, 0.07);
  border-bottom: 1px solid #dfdfdf;
}
.tab-bar .menu-icon{
	text-indent: 0;
}
.tab-bar .menu-icon i {
  color: #666;
  font-size: 25px;
  left: 15px;
  position: absolute;
  top: 25%;
}
.tab-bar .menu-icon i span {
  font-family: "Open Sans Condensed",Helvetica,sans-serif;
  font-weight: 700;
  padding: 0 8px;
}
.xskin-dashboard-menu .menu-icon i span::after {
  content: none;
}

/* right menut */
.xskin-dashboard-menu ul{
	padding: 0 15px;
	float: right;
  position: relative;
}
.xskin-dashboard-menu ul li {
  border-left: 1px solid #dfdfdf;
  border-right: 1px solid #fff;
  float: left;
  height: 45px;
  line-height: 45px;
  width: auto;
  position: relative;
  transition: all 300ms ease-in-out 0.0s;
  -o-transition: all 300ms ease-in-out 0.0s;
  -ms-transition: all 300ms ease-in-out 0.0s;
  moz-transition: all 300ms ease-in-out 0.0s;
  -webkit-transition: all 300ms ease-in-out 0.0s;
}
.xskin-dashboard-menu ul > li:hover,
.xskin-dashboard-menu ul > li:focus {
  background: #f7f7f7 none repeat scroll 0 0;
}
.xskin-dashboard-menu ul li:first-child{
	border-left: 0 none;
}
.xskin-dashboard-menu ul li:last-child,
.xskin-dashboard-menu ul li #drop li:last-child a{
	border-right: 0 none;
}
.xskin-dashboard-menu ul li a {
  color: #666;
  font-size: 15px;
  font-weight: 600;
  padding: 0 15px;
}
.xskin-dashboard-menu ul li span{
	color: #666;
	cursor: pointer;
	padding: 0 20px;
	position: relative;
  display: inline-block;
}
.xskin-dashboard-menu ul li span span {
  background-color: #de127a;
  border-radius: 50px;
  color: #fff;
  font-family: arial;
  font-size: 11px;
  height: 17px;
  line-height: 17px;
  padding: 0;
  position: absolute;
  right: 10px;
  text-align: center;
  top: 4px;
  width: 17px;
}
ul.xskin-dropdown {
  background-color: #fff;
  border-bottom: 1px solid #ddd;
  border-left: 1px solid #ddd;
  border-right: 1px solid #ddd;
  margin: 0;
  padding: 0;
  position: absolute;
  right: 0;
  text-align: left;
  top: 100%;
  width: 370px;
  z-index: 89;
  max-height: 365px;
  overflow: auto;
  transform: scale(0);
  -o-transform: scale(0);
  -ms-transform: scale(0);
  -moz-transform: scale(0);
  -webkit-transform: scale(0);
}
.xskin-dropdown.active{
  transform: scale(1);
  -o-transform: scale(1);
  -ms-transform: scale(1);
  -moz-transform: scale(1);
  -webkit-transform: scale(1);
}
.xskin-dropdown.active::before {
  content: url("../assets/cater.png");
  height: 8px;
  position: absolute;
  right: 24px;
  top: -19px;
  width: 15px;
}
ul.xskin-dropdown li {
  border: 0 none;
  float: none;
  line-height: normal;
  height: auto;
  padding: 0;
  width: 100%;
}
ul.xskin-dropdown li a {
  padding: 0 15px;
  color: #666;
  font-weight: 700;
  float: left;
  width: 100%;
  padding:10px;
  border-bottom: 1px #ddd solid;
}
ul.xskin-dropdown li:hover a{
  background-color: #ebebeb;
}
.profile-pic {
  border-radius: 50px;
  float: left;
  height: 40px;
  overflow: hidden;
  width: 40px;
}
.notification-title {
  float: right;
  max-width: 275px;
}
ul.xskin-dropdown li .notification-title span {
  display: inline-block;
  font-size: 14px;
  font-weight: 300;
  line-height: 18px;
  padding: 0;
  width: 100%;
}
ul.xskin-dropdown li .notification-title span.name{
  font-weight: 700;
  line-height: 25px;
}

/** dashboard content **/
.xskin-dshboard-design {
  padding: 20px;
}
.xskin-dshboard-design h3 {
  background: #ff9f18 none repeat scroll 0 0;
  border-bottom: 5px solid #ff8400;
  border-radius: 3px;
  color: #fff;
  font-size: 27px;
  font-weight: 600;
  padding: 0 20px;
}
.xskin-dshboard-design .breadcrumbs {
  background-color: rgba(0, 0, 0, 0.07);
  margin: 20px 0 0;
  padding: 9px 15px;
}
.xskin-dshboard-design .breadcrumbs > *{

}
ul.breadcrumbs li a,
.xskin-dshboard-design .breadcrumbs > *::before,
.xskin-dshboard-design .breadcrumbs > * {
  color: #666;
  font-family: "Open Sans Condensed",Helvetica,sans-serif;
  font-size: 13px;
}
ul.breadcrumbs li a {
  font-weight: 600;
  padding: 0 5px 0 0;
}
.xskin-dshboard-design .breadcrumbs > *::before{
  padding-right: 5px;
}
.xskin-users-entry {
  background-color: rgba(0, 0, 0, 0.06);
  border: 1px solid rgba(0, 0, 0, 0.1);
  display: inline-block;
  margin: 20px 0 0;
  padding: 20px;
  width: 100%;
}
.xskin-users-entry .tabs {
  background-color: #888;
}
.xskin-users-entry .tab-title{
  border-right: 1px solid #fff;
}
.xskin-users-entry .tab-title a {
  background-color: transparent;
  color: #fff;
  height: 42px;
  line-height: 42px;
  padding: 0 30px;
}
.xskin-users-entry .tabs .tab-title.active > a,
.xskin-users-entry .tabs .tab-title:hover > a{
  background-color: #ff9f18;
  color: #fff;
}
.xskin-users-entry .tabs-content > .content {
  background-color: #fff;
  padding: 15px 20px;
}
.xskin-users-entry .refresh-list form input {
  float: left;
  margin: 0;
  padding: 0 15px;
  width: 50%;
}
.xskin-users-entry .refresh-list form button {
  background-color: #ff9f18;
  height: 38px;
  padding: 0 15px;
  float: left;
  margin: 0;
}
.xskin-users-entry .refresh-list form span {
  background-color: rgba(0, 0, 0, 0.44);
  color: #fff;
  cursor: pointer;
  display: inline-block;
  float: left;
  height: 38px;
  line-height: 38px;
  padding: 0 10px;
}
.refresh-entry,
.found-user{
  float: left;
  width: 50%;
}
.found-user a.button{
  padding: 0 25px;
  width: auto;
  height: 40px;
  line-height: 40px;
}
.xskin-user-entry{
  margin: 10px 0 0;
}
.admin-alert-box.success, .admin-alert-box.alert{
    background-color: #dff0d8;
    border-color: #d6e9c6;
    color: #3c763d;
    font-size: 20px;
    position: relative;
}
.admin-alert-box.success .close {
    color: #598D59 !important;
}
.label-success {
    background-color: #5cb85c;
}
.label-danger {
    background-color: #d9534f;
}
.admin-alert-box .close {
    right: 0.25rem;
    background: inherit;
    color: #598D59;
    font-size: 1.375rem;
    line-height: .9;
    margin-top: -0.6875rem;
    opacity: 1;
    padding: 0 6px 4px;
    position: absolute;
    top: 50%;
}
.xskin-user-entry ul li.top-heading-strip {
  background-color: rgba(0, 0, 0, 0.05);
  display: inline-block;
  padding: 5px 7px;
  width: 100%;
  margin: 0 0 5px;
}
table .last.shrink {
    width: 1px;
    white-space: nowrap;
}
.xskin-user-entry table tr,
.xskin-user-entry table th,
.xskin-user-entry table td{
  padding: 10px 5px;
}
.xskin-user-entry table tr:nth-child(2n+2) {
  background-color: rgba(0, 0, 0, 0.05);
}
.xskin-user-entry table input{
  margin: 0;
}
.xskin-user-entry table th{
  color: #666;
}
.xskin-user-entry table td{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 150px;
}
.xskin-user-entry table td.links{
  width: 70px;
}
.xskin-user-entry table td.links ul li{
  float: left;
  padding: 0 4px;
}
.xskin-user-entry table td.links ul li a{
  color: #666;
  font-size: 14px;
}
.xskin-add-new-user form.new-user-ad p{
  margin: 0 0 5px;
  color: #666;
  font-size: 13px;
  font-family: arial;
}
.xskin-add-new-user form.new-user-ad label,
.xskin-add-new-user form.new-user-ad label input{
  line-height: 1;
  margin: 0 3px 0 0;
  vertical-align: top;
}
.xskin-action{
  margin: 10px 0;
  display: inline-block;
  width: 100%;
  padding: 0 15px;
}
.xskin-action a{
  float: left;
  margin: 0 10px 0 0;
}
.xskin-action a.cancel{
  background-color: #888;
}

/** basic info **/
.user-info {
  background-color: rgba(0, 0, 0, 0.09);
  border-radius: 3px;
  display: inline-block;
  max-width: 500px;
  padding: 20px;
  margin: 20px 0;
  border-bottom: 5px solid rgba(0, 0, 0, 0.09);
}
.xskin-user-basic-information h4{
  color: #666;
  font-weight: 700;
  margin: 0 0 20px;
  line-height: 1;
}
.user-info span {
  border-bottom: 1px dashed #fff;
  float: left;
  margin: 10px 0 0;
  padding: 0 0 10px;
  color: #666;
  width: 100%;
}
.user-infor{
  font-weight: 700;
}