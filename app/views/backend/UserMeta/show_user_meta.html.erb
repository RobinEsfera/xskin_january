
      <!-- xskin dashboard design start from here -->
      <div class="xskin-dshboard-design">
        <h3>Detail of User Meta "#<%= @user_meta.id %>"</h3>
        <ul class="breadcrumbs">
            <li><a href="#">Dashboard</a></li>
            <li><a href="/backend/user_meta">User Metas</a></li>
            <li class="current">Show</li>
        </ul>

        <!-- user entry start from here -->
        <div class="xskin-users-entry">
            <ul class="tabs">
            <li class="tab-title active"><a href="show_user_meta"><i class="fa fa-camera"></i> Show</a></li>
            <li class="tab-title"><a href="edit_user_meta"><i class="fa fa-pencil"></i> Edit</a></li>
            <li class="tab-title">
              <%= link_to "<i class='fa fa-trash'></i> Delete".html_safe, "delete_user_meta", method: :delete, :title => "Delete", data: { confirm: 'Are you confirm you want to delete this?' } %>
            </li>
          </ul>

          <div class="tabs-content">

  <!-- user information Start from here -->
             <div id="Show" class="content active">
                <div class="xskin-user-basic-information">
                  <div class="user-info">
                    <h4>Basic information</h4>

                    <div class="medium-4 left">
                      <span class="type-info">
                        User
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                      <% if !@user_meta.nil? %>
                        <% user = User.where(:id => @user_meta.user_id).first %>
                      <% end %>
                        <%= (!user.nil?) ? user.name : "-" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Currency
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (@user_meta.currency.present?) ? @user_meta.currency : "-" %>
                      </span>
                    </div>


                    <div class="medium-4 left">
                      <span class="type-info">
                        Created At
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@user_meta.nil?) ? @user_meta.created_at.to_formatted_s(:long) : "" %>
                      </span>
                    </div>

                  </div><!-- /user-info -->
                </div><!-- basic information close -->

                <div class="clearfix"></div>
             </div>
             <!-- user information close from here -->

          </div>

        </div>
        <!-- user entry close from here -->

      </div>
      <!-- xskin dashboard design close from here -->

