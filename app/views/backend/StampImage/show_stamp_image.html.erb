
      <!-- xskin dashboard design start from here -->
      <div class="xskin-dshboard-design">
        <h3>Detail of Stamp Image "#<%= @stamp_image.id %>"</h3>
        <ul class="breadcrumbs">
            <li><a href="#">Dashboard</a></li>
            <li><a href="/backend/stamp_image">Stamp Images</a></li>
            <li class="current">Show</li>
        </ul>

        <!-- user entry start from here -->
        <div class="xskin-users-entry">
            <ul class="tabs">
            <li class="tab-title active"><a href="show_stamp_image"><i class="fa fa-camera"></i> Show</a></li>
            <li class="tab-title"><a href="edit_stamp_image"><i class="fa fa-pencil"></i> Edit</a></li>
            <li class="tab-title">
              <%= link_to "<i class='fa fa-trash'></i> Delete".html_safe, "delete_stamp_image", method: :delete, :title => "Delete", data: { confirm: 'Are you confirm you want to delete this?' } %>
            </li>
          </ul>

          <div class="tabs-content">

  <!-- user information Start from here -->
             <div id="Show" class="content active">
                <div class="xskin-user-basic-information">
                  <div class="user-info">
                    <h4>Basic information</h4>

                    <div class="medium-4 left">
                      <span class="type-info">
                        Stamp
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                      <% if !@stamp_image.nil? %>
                        <% stamp = Stamp.where(:id => @stamp_image.stamp_id).first %>
                      <% end %>
                        <%= (!stamp.nil?) ? stamp.name : "-" %>
                      </span>
                    </div>
                    <div class="medium-12 left">
                        <span class="type-info">
                      <div class="medium-4 left">
                          Image
                      </div>
                      <div class="medium-8 left">
                          <%= (@stamp_image.image.present?) ? image_tag(@stamp_image.image.url(:thumb)) : "" %>
                      </div>
                        </span>
                    </div>


                    <div class="medium-4 left">
                      <span class="type-info">
                        Created At
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@stamp_image.nil?) ? @stamp_image.created_at.to_formatted_s(:long) : "" %>
                      </span>
                    </div>

                  </div><!-- /user-info -->
                </div><!-- basic information close -->

                <div class="clearfix"></div>
             </div>
             <!-- user information close from here -->

          </div>

        </div>
        <!-- user entry close from here -->

      </div>
      <!-- xskin dashboard design close from here -->

