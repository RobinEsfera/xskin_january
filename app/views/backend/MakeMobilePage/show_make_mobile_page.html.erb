
      <!-- xskin dashboard design start from here -->
      <div class="xskin-dshboard-design">
        <h3>Detail of Make Mobile Page "<%= @make_mobile_page.title %>"</h3>
        <ul class="breadcrumbs">
            <li><a href="#">Dashboard</a></li>
            <li><a href="/backend/make_mobile_page">Make Mobile Pages</a></li>
            <li class="current">Show</li>
        </ul>

        <!-- user entry start from here -->
        <div class="xskin-users-entry">
            <ul class="tabs">
            <li class="tab-title active"><a href="show_make_mobile_page"><i class="fa fa-camera"></i> Show</a></li>
            <li class="tab-title"><a href="edit_make_mobile_page"><i class="fa fa-pencil"></i> Edit</a></li>
            <li class="tab-title">
              <%= link_to "<i class='fa fa-trash'></i> Delete".html_safe, "delete_make_mobile_page", method: :delete, :title => "Delete", data: { confirm: 'Are you confirm you want to delete this?' } %>
            </li>
          </ul>

          <div class="tabs-content">

  <!-- user information Start from here -->
             <div id="Show" class="content active">
                <div class="xskin-user-basic-information">
                  <div class="user-info">
                    <h4>Basic information</h4>

                    <div class="medium-4 left">
                      <span class="type-info">
                        Title
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (@make_mobile_page.title.present?) ? @make_mobile_page.title : "-" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Title
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (@make_mobile_page.dowanload_button_text.present?) ? @make_mobile_page.dowanload_button_text : "-" %>
                      </span>
                    </div>
                    <div class="medium-12 left">
                        <span class="type-info">
                      <div class="medium-4 left">
                          App Icon
                      </div>
                      <div class="medium-8 left">
                          <%= (@make_mobile_page.app_icon.present?) ? image_tag(@make_mobile_page.app_icon.url()) : "-" %>
                      </div>
                        </span>
                    </div>
                    <div class="medium-12 left">
                        <span class="type-info">
                      <div class="medium-4 left">
                          Background
                      </div>
                      <div class="medium-8 left">
                          <%= (@make_mobile_page.background.present?) ? image_tag(@make_mobile_page.background.url()) : "-" %>
                      </div>
                        </span>
                    </div>

                    <div class="medium-4 left">
                      <span class="type-info">
                        Created At
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@make_mobile_page.nil?) ? @make_mobile_page.created_at.to_formatted_s(:long) : "" %>
                      </span>
                    </div>

                  </div><!-- /user-info -->
                </div><!-- basic information close -->

                <div class="clearfix"></div>
             </div>
             <!-- user information close from here -->

          </div>

        </div>
        <!-- user entry close from here -->

      </div>
      <!-- xskin dashboard design close from here -->

