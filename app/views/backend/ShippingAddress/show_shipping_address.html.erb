
      <!-- xskin dashboard design start from here -->
      <div class="xskin-dshboard-design">
        <h3>Detail of Shipping Address "<%= @shipping_address.first_name %>"</h3>
        <ul class="breadcrumbs">
            <li><a href="#">Dashboard</a></li>
            <li><a href="/backend/shipping_address">Shipping Address</a></li>
            <li class="current">Show</li>
        </ul>

        <!-- user entry start from here -->
        <div class="xskin-users-entry">
            <ul class="tabs">
            <li class="tab-title active"><a href="show_shipping_address"><i class="fa fa-camera"></i> Show</a></li>
            <li class="tab-title"><a href="edit_shipping_address"><i class="fa fa-pencil"></i> Edit</a></li>
            <li class="tab-title">
              <%= link_to "<i class='fa fa-trash'></i> Delete".html_safe, "delete_shipping_address", method: :delete, :title => "Delete", data: { confirm: 'Are you confirm you want to delete this?' } %>
            </li>
          </ul>

          <div class="tabs-content">

  <!-- user information Start from here -->
             <div id="Show" class="content active">
                <div class="xskin-user-basic-information">
                  <div class="user-info">
                    <h4>Basic information</h4>

                    <div class="medium-4 left">
                      <span class="type-info">
                        User
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                      <% if !@shipping_address.nil? %>
                        <% shipping_user = User.where(:id => @shipping_address.user_id).first %>
                      <% end %>
                        <%= (!shipping_user.nil?) ? shipping_user.name : "sdf" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        First Name
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.first_name : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Last Name
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.last_name : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Address
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.address1 : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Country
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                      <% if !@shipping_address.nil? %>
                        <% shipping_country = Country.where(:id => @shipping_address.country_id).first %>
                      <% end %>
                        <%= (!shipping_country.nil?) ? shipping_country.name : "sdf" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        City
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.city : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        State
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.state : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Pincode
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.pincode : "" %>
                      </span>
                    </div>
                    <div class="medium-4 left">
                      <span class="type-info">
                        Mobile
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.mobile : "" %>
                      </span>
                    </div>


                    <div class="medium-4 left">
                      <span class="type-info">
                        Created At
                      </span>
                    </div>
                    <div class="medium-8 left">
                      <span class="user-infor">
                        <%= (!@shipping_address.nil?) ? @shipping_address.created_at.to_formatted_s(:long) : "" %>
                      </span>
                    </div>

                  </div><!-- /user-info -->
                </div><!-- basic information close -->

                <div class="clearfix"></div>
             </div>
             <!-- user information close from here -->

          </div>

        </div>
        <!-- user entry close from here -->

      </div>
      <!-- xskin dashboard design close from here -->

